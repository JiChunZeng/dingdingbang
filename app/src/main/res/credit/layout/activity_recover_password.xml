<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>
        <variable
            name="recoverPasswordActivity"
            type="com.dingtaifu.dingdingbang.mvp.credit.view.RecoverPasswordActivity" />
    </data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bottom_background"
        android:orientation="vertical"
        tools:context=".mvp.credit.view.RecoverPasswordActivity">

        <include
            android:id="@+id/layout_common_title"
            layout="@layout/layout_common_title" />

        <include
            android:id="@+id/layout_recover_name_id"
            layout="@layout/layout_recover_name_id_loginname" />

        <include
            android:id="@+id/layout_recover_password_phone"
            layout="@layout/layout_recover_password_phone" />
        <include
            android:id="@+id/layout_recover_question"
            layout="@layout/layout_recover_questions"></include>
        <include
            android:id="@+id/layout_recover_web"
            layout="@layout/layout_recover_web"></include>

        <include
            android:id="@+id/layout_recover_banggain"
            layout="@layout/layout_recover_banggain" />
    </LinearLayout>
</layout>
